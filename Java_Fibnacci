class Solution {
    public int fib(int n) {
        if (n ==0 ) return 0;
        if (n ==1 ) return 1;

        //fn2 fn1 fn
        //每次求和后右移
        int fn2 = 0;
        int fn1 = 1;
        int fn = 0;
        for(int i = 0; i <= n; i++) {
            fn = (fn2 + fn1);
            fn2 = fn1;
            fn1 = fn;
        }
        return fn;
    }
}
